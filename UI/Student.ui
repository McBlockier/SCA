          <property name="styleSheet">
           <string notr="true">/* Estilo base del QProgressBar */
QProgressBar {
    background-color: #e0abff;
    height: 5px; /* Incrementa el tamaño vertical para una mejor apariencia */
    border-radius: 3px; /* Ajusta el radio de borde para una apariencia más suave */
}

/* Estilo del chunk (parte de progreso) */
QProgressBar::chunk {
    background-color: white; /* Cambia el color del chunk */
    border-radius: 3px; /* Ajusta el radio de borde del chunk */
}

/* Añade transiciones para hacer los cambios de estilo más suaves */
QProgressBar {
    transition: background-color 0.3s ease;
}

QProgressBar::chunk {
    transition: background-color 0.3s ease;
}
