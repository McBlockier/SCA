         <property name="styleSheet">
          <string notr="true">QFrame {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                 stop:0 #fada38,
                                 stop:0.5 #ffd700,
                                 stop:1 #fada38);
    border-radius: 15px;
    position: relative; /* Asegura que el seudoelemento se posicione correctamente */
    overflow: hidden; /* Evita que el degradado se desborde fuera del QFrame */
}

QFrame::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                 stop:0 #fada38,
                                 stop:0.5 #ffd700,
                                 stop:1 #fada38);
    border-radius: inherit; /* Hereda el radio de borde del QFrame */
    transition: opacity 0.3s ease; /* Transición suave para la opacidad */
    opacity: 0; /* Inicialmente oculta el degradado */
}

QFrame:hover::before {
    opacity: 1; /* Muestra el degradado al hacer hover */
}
</string>
